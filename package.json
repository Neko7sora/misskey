{
	"name": "misskey",
	"version": "13.13.2",
	"codename": "nasubi",
	"repository": {
		"type": "git",
		"url": "https://github.com/misskey-dev/misskey.git"
	},
	"packageManager": "pnpm@8.6.0",
	"workspaces": [
		"packages/frontend",
		"packages/backend",
		"packages/sw"
	],
	"private": true,
	"scripts": {
		"api": "cross-env FORCE_COLOR=1 turbo run api",
		"api-prod": "cross-env FORCE_COLOR=1 turbo run api-prod",
		"build": "run-s build:pre build:main build:post",
		"build:main": "cross-env FORCE_COLOR=1 turbo run build:main",
		"build:post": "turbo run build:post",
		"build:pre": "turbo run build:pre",
		"build-storybook": "turbo run build-storybook",
		"check:connect": "turbo run check:connect",
		"clean": "turbo run clean",
		"clean-all": "turbo run clean-all",
		"cleanall": "run-s clean-all",
		"cy:open": "pnpm cypress open --browser --e2e --config-file=cypress.config.ts",
		"cy:run": "pnpm cypress run",
		"dev": "run-s dev:setup dev:main",
		"dev:main": "cross-env FORCE_COLOR=1 turbo run dev:main",
		"dev:main:swc": "cross-env FORCE_COLOR=1 turbo run dev:main:swc",
		"dev:setup": "run-s clean build",
		"dev:watch": "turbo run dev:watch",
		"dev:watch:swc": "turbo run dev:watch:swc",
		"e2e": "pnpm start-server-and-test 'turbo run start:test' http://localhost:61812 cy:run",
		"init": "cross-env FORCE_COLOR=1 run-s migrate",
		"jest": "cross-env FORCE_COLOR=1 turbo run jest",
		"jest-and-coverage": "turbo run jest -- --coverage",
		"jest-clear": "turbo run jest-clear",
		"lint": "cross-env FORCE_COLOR=1 turbo run lint",
		"migrate": "cross-env FORCE_COLOR=1 turbo run migrate",
		"migrateandstart": "run-s migrate start",
		"start": "cross-env FORCE_COLOR=1 turbo run start",
		"start:test": "cross-env FORCE_COLOR=1 NODE_ENV=test turbo run start",
		"test": "cross-env FORCE_COLOR=1 turbo run test",
		"test-and-coverage": "cross-env FORCE_COLOR=1 turbo run test -- --coverage",
		"tsd": "cross-env FORCE_COLOR=1 turbo run tsd",
		"vitest": "cross-env FORCE_COLOR=1 turbo run vitest",
		"vitest-and-coverage": "cross-env FORCE_COLOR=1 turbo run vitest -- --coverage"
	},
	"resolutions": {
		"chokidar": "3.5.3",
		"lodash": "4.17.21"
	},
	"dependencies": {
		"execa": "5.1.1",
		"js-yaml": "4.1.0",
		"turbo": "^1.10.3",
		"typescript": "5.1.3"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "5.59.8",
		"@typescript-eslint/parser": "5.59.8",
		"cross-env": "7.0.3",
		"cypress": "12.13.0",
		"eslint": "8.41.0",
		"npm-run-all": "^4.1.5",
		"start-server-and-test": "2.0.0"
	},
	"optionalDependencies": {
		"@tensorflow/tfjs-core": "4.4.0"
	}
}

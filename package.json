{
	"name": "misskey",
	"version": "13.6.1",
	"codename": "nasubi",
	"repository": {
		"type": "git",
		"url": "https://github.com/misskey-dev/misskey.git"
	},
	"packageManager": "pnpm@7.27.0",
	"workspaces": [
		"packages/frontend",
		"packages/backend",
		"packages/sw"
	],
	"private": true,
	"scripts": {
		"build": "run-s build-pre build-mid build-post",
		"build-mid": "turbo run build",
		"build-pre": "turbo run build-pre",
		"build-post": "turbo run build-post",
		"clean": "turbo run clean",
		"clean-all": "run-s clean-all",
		"cleanall": "turbo run clean-all",
		"cy:open": "cypress open --browser --e2e --config-file=./../cypress.config.ts",
		"cy:run": "cypress run",
		"dev": "turbo run dev",
		"e2e": "start-server-and-test start:test http://localhost:61812 cy:run",
		"init": "turbo run init",
		"jest": "turbo run jest",
		"jest-and-coverage": "turbo run jest-and-coverage",
		"jest-clear": "turbo run jest-clear",
		"lint": "turbo run lint",
		"migrate": "turbo run migrate",
		"migrateandstart": "turbo run migrateandstart",
		"start": "turbo run start",
		"start:test": "turbo run start:test",
		"test": "run-s jest",
		"test-and-coverage": "run-s jest-and-coverage",
		"watch": "turbo run watch"
	},
	"resolutions": {
		"chokidar": "3.5.3",
		"lodash": "4.17.21"
	},
	"dependencies": {
		"js-yaml": "4.1.0",
		"typescript": "4.9.5",
	},
	"devDependencies": {
    "@types/gulp": "4.0.10",
    "@types/gulp-rename": "2.0.1",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "cross-env": "7.0.3",
    "cypress": "12.5.1",
    "eslint": "8.33.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "1.15.3",
    "turbo": "^1.7.4"
},
	"optionalDependencies": {
		"@tensorflow/tfjs-core": "4.2.0"
	}
}
